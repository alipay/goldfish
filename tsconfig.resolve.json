      "@goldfishjs/reactive": ["./packages/reactive/src/index.ts"],
      "@goldfishjs/reactive/lib/*": ["./packages/reactive/src/*"],
      "@goldfishjs/requester": ["./packages/requester/src/index.ts"],
      "@goldfishjs/requester/lib/*": ["./packages/requester/src/*"],
      "@goldfishjs/route": ["./packages/route/src/index.ts"],
      "@goldfishjs/route/lib/*": ["./packages/route/src/*"],
      "@goldfishjs/test-utils": ["./packages/test-utils/src/index.ts"],
      "@goldfishjs/test-utils/lib/*": ["./packages/test-utils/src/*"],
      "@goldfishjs/utils": ["./packages/utils/src/index.ts"],
      "@goldfishjs/utils/lib/*": ["./packages/utils/src/*"]
    }
  }
}
